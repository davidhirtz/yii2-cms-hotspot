{
  "name": "davidhirtz/yii2-cms-hotspot",
  "type": "yii2-extension",
  "description": "Yii2 Image Hotspot Module",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-media": "*",
    "davidhirtz/yii2-cms": "*"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\cms\\hotspot\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\cms\\hotspot\\composer\\Bootstrap"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-cms-hotspot.git"
    }
  ]
}